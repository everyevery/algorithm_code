public class Solution {
    public int findDuplicate(int[] nums) {
        return binarySearch(nums);
    }

    private int binarySearch(int[] nums) {
        int low = 1;
        int high = nums.length;

        while (low < high) {
            int mid = (low + high) / 2;
            int count = 0;
            for (int n : nums) {
                if (low <= n && n <= mid) count++;
            }
            if (count > (mid - low + 1)) {
                high = mid;
            } else {
                low = mid + 1;
            }
        }
        return low;
    }
}
